{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "desktop-capability",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "platforms": ["linux", "macOS"],
  "remote": {
    "urls": [
      "http://*",
      "https://*"
    ]
  },
  "permissions": [
    "core:default",
    "videoplayer:default",
    "os:default",
    "core:window:allow-set-fullscreen"
  ]
}